    "    \n",
    "    for r in reps:\n",
    "        rep = r.split(\"_\")[1]\n",
    "        \n",
    "        rna_barc_len = sum(~pd.isnull(df[r]))\n",
    "        rna_barc_perc = (float(rna_barc_len)/index_len)*100\n",
    "        \n",
    "        print(\"\\trep %s: %s barcodes at RNA level (%s%%)\" % (rep, rna_barc_len, rna_barc_perc))\n",
    "    print(\"\")"
